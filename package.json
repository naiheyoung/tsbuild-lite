{
  "name": "@naiheyoung/tsbuild-lite",
  "version": "1.0.0",
  "packageManager": "pnpm@10.13.1",
  "description": "A TypeScript library template with CJS + ESM + CLI support",
  "homepage": "https://github.com/naiheyoung/tsbuild-lite#readme",
  "author": "Naihe",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./cli": {
      "import": "./dist/cli.js",
      "require": "./dist/cli.cjs"
    }
  },
  "bin": {
    "hello": "bin/hello.js"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm run clean && tsup"
  },
  "keywords": [
    "tsbuild-lite",
    "typescript",
    "esm",
    "cjs",
    "cli",
    "template"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "devDependencies": {
    "@types/node": "^22.10.0",
    "cac": "^6.7.14",
    "chalk": "^5.5.0",
    "ora": "^8.2.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.7.2"
  },
  "publishConfig": {
    "access": "public"
  }
}